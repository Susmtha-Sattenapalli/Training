{
    "manifest_version": 3,
    "name": "LinkedIn Profile Pic Changer",
    "version": "1.0",
    "description": "A Chrome extension to change LinkedIn profile pictures in posts to a specified image.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/feed/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "128": "icon.png"
    }
  }
  